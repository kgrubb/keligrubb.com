    "babel-eslint": "^10.0.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-vue": "^7.6.0",
    "husky": "4",
